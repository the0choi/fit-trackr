<%- include('../partials/header') %>

<h3 class="text-3xl font-bold underline">Dashboard</h3>

<table id="list">
    <thead>
      <tr>
        <th>Date</th>
        <th>Calories Consumed</th>
        <th>Calories Burned</th>
      </tr>
    </thead>
    <tbody>
      <% health.forEach(function(h) { %>
        <tr>
          <td><%= h.date %></td>
          <td><%= h.caloriesIn %></td>
          <td><%= h.caloriesOut %></td>
          <td><a href="/health/<%= h._id %>">Details</a></td>
        </tr>
        <% }); %>
      </tbody>
</table>

<%- include('../partials/footer') %>